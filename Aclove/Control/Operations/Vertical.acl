let seg = !../Signals/Segment in
let mul = !../Signals/Multiplication in
let str = !../Signals/Stretch in
let dur = !../Signals/Duration in

'vertical [
    @ %sc %s -> %s <mul> str (dur %s) (seg %sc %sc)
]

